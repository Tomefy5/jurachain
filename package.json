{
    "name": "justice-automation",
    "version": "1.0.0",
    "description": "Plateforme décentralisée basée sur l'IA et la blockchain pour l'accès aux documents légaux à Madagascar",
    "main": "src/server.js",
    "scripts": {
        "dev": "nodemon src/server.js",
        "start": "node src/server.js",
        "build": "npm run build:react && tsc",
        "build:react": "npx webpack --mode production",
        "dev:react": "npx webpack serve --mode development",
        "test": "jest",
        "test:coverage": "jest --coverage --watchAll=false",
        "monitoring:start": "./scripts/start-monitoring.sh",
        "monitoring:stop": "docker-compose down prometheus alertmanager node-exporter blackbox-exporter sonarqube sonarqube-db",
        "sonar:analyze": "./scripts/sonar-analysis.sh",
        "metrics": "curl -s http://localhost:3000/metrics",
        "health": "curl -s http://localhost:3000/health/detailed | jq ."
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@hashgraph/sdk": "^2.76.0",
        "@supabase/supabase-js": "^2.38.4",
        "@tensorflow/tfjs-node": "^4.15.0",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@types/react-router-dom": "^5.3.3",
        "axios": "^1.13.1",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "ethers": "^6.15.0",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "googleapis": "^164.1.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "ollama": "^0.6.2",
        "prom-client": "^15.1.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-router-dom": "^7.9.5",
        "react-scripts": "^5.0.1",
        "socket.io": "^4.8.1",
        "styled-jsx": "^5.1.7",
        "uuid": "^8.3.2",
        "ws": "^8.18.3",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/node": "^20.10.4",
        "css-loader": "^7.1.2",
        "html-webpack-plugin": "^5.6.4",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "style-loader": "^4.0.0",
        "supertest": "^6.3.3",
        "ts-loader": "^9.5.4",
        "typescript": "^5.9.3",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2"
    },
    "keywords": [
        "legal",
        "blockchain",
        "ai",
        "madagascar",
        "contracts"
    ],
    "author": "JusticeAutomation Team",
    "license": "MIT"
}